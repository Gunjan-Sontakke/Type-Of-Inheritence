package Inheritence;

public class Bank {

	public static void main(String[] args) {
		System.out.println(Bank1.i);
		System.out.println(urbanbank.j);
		System.out.println(sbi.ch);

	}
	
	public static class Bank1{
		static int i=12;
		
	}
	class urbanbank extends Bank1{
		static int j= 13;
		
	}
	class sbi extends Bank{
		static String ch="selenium";
		
	
	}
	public class ruralbank{
		
	}
	class jsbk extends ruralbank{
		
			
	}
}
